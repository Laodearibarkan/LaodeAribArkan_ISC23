package Day32_ISC;

import java.util.Scanner;

public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Masukkan angka : ");
        int a = input.nextInt();
        for(i = 1; i <= a; i++){
            for(j = 1; j < a; j++){
                System.out.print("o ");
            }
            System.out.println("o ");
            
        }
        System.out.println("");
    }
    
}
